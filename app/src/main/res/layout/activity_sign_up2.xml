        android:layout_height="45dp"
        android:layout_marginTop="48dp"
        android:width="150dp"
        android:height="0dp"
        android:background="@drawable/btn_style"
        android:padding="10dp"
        android:text="SUBMIT"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="@+id/gluten_checkbox_input"
        app:layout_constraintTop_toBottomOf="@+id/chicken_checkbox_input" />

