        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="@+id/saturated_fat_title"
        app:layout_constraintTop_toBottomOf="@+id/saturated_fat_title" />

    <SeekBar
        android:id="@+id/sodium_seekBar_input"
        android:layout_width="140dp"
        android:layout_height="21dp"
        android:layout_marginTop="16dp"
        android:max="10"
        android:progress="0"
        android:progressBackgroundTint="@color/white"
        android:progressTint="@color/brown"
        android:scaleX="1.2"
        android:scaleY="1.2"
        android:thumbTint="@color/brown"
        app:layout_constraintStart_toStartOf="@+id/saturated_fat_seekBar_input"
        app:layout_constraintTop_toBottomOf="@+id/saturated_fat_seekBar_input" />

    <TextView
        android:id="@+id/saturated_fat_title"
