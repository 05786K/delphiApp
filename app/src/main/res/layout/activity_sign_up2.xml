<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray"
    tools:layout_editor_absoluteX="1dp"
    tools:layout_editor_absoluteY="32dp">


    <CheckBox
        android:id="@+id/chicken_checkbox_input"
        android:layout_width="116dp"
        android:layout_height="31dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="12dp"
        android:width="150dp"
        android:buttonTint="@color/brown"
        android:text="Chicken"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@+id/gluten_checkbox_input"
        app:layout_constraintTop_toBottomOf="@+id/nuts_checkbox_input" />

    <CheckBox
        android:id="@+id/gluten_checkbox_input"
        android:layout_width="99dp"
        android:layout_height="33dp"
        android:layout_marginTop="12dp"
        android:width="150dp"
        android:buttonTint="@color/brown"
        android:text="Gluten"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="@+id/vegan_checkbox_input"
        app:layout_constraintTop_toBottomOf="@+id/vegan_checkbox_input" />

    <CheckBox
        android:id="@+id/lactose_checkbox_input"
        android:layout_width="111dp"
        android:layout_height="31dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="16dp"
        android:width="150dp"
        android:buttonTint="@color/brown"
        android:text="Lactose"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@+id/nuts_checkbox_input"
        app:layout_constraintTop_toBottomOf="@+id/preferences_title3" />

    <CheckBox
        android:id="@+id/nuts_checkbox_input"
        android:layout_width="99dp"
        android:layout_height="32dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:width="150dp"
        android:buttonTint="@color/brown"
        android:text="Nuts"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@+id/vegan_checkbox_input"
        app:layout_constraintTop_toBottomOf="@+id/preferences_title3" />

    <CheckBox
        android:id="@+id/vegan_checkbox_input"
        android:layout_width="99dp"
        android:layout_height="31dp"
        android:layout_marginTop="16dp"
        android:width="150dp"
        android:buttonTint="@color/brown"
        android:text="Vegan"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="@+id/preferences_title3"
        app:layout_constraintTop_toBottomOf="@+id/preferences_title3" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="242dp"
        android:layout_height="57dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="20dp"
        android:text="Registration"
        android:textColor="@color/white"
        android:textSize="40dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/first_name_input"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="22dp"
        android:width="280dp"
        android:backgroundTint="@color/brown"
        android:backgroundTintMode="add"
        android:ems="10"
        android:hint="First Name"
        android:inputType="textPersonName"
        android:soundEffectsEnabled="false"
        android:textColorHint="@color/white"
        android:textSize="22dp"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3"></EditText>

    <EditText
        android:id="@+id/last_name_input"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:width="280dp"
        android:backgroundTint="@color/brown"
        android:backgroundTintMode="add"
        android:ems="10"
        android:hint="Last Name"
        android:inputType="textPersonName"
        android:textColorHint="@color/white"
        android:textSize="22dp"
        app:layout_constraintStart_toStartOf="@+id/first_name_input"
        app:layout_constraintTop_toBottomOf="@+id/first_name_input"></EditText>

    <EditText
        android:id="@+id/password_input"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:width="280dp"
        android:backgroundTint="@color/brown"
        android:backgroundTintMode="add"
        android:ems="10"
        android:hint="Password"
        android:inputType="textPassword"
        android:textColorHint="@color/white"
        android:textSize="22dp"
        app:layout_constraintStart_toStartOf="@+id/email_input"
        app:layout_constraintTop_toBottomOf="@+id/email_input"></EditText>

    <TextView
        android:id="@+id/preferences_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginTop="52dp"
        android:text="Provide Preferences Below:"
        android:textColor="@color/white"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/message1"
        app:layout_constraintTop_toBottomOf="@+id/message1" />

    <TextView
        android:id="@+id/preferences_title3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginTop="28dp"
        android:text="Check Food Allergies:"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="@+id/protein_title"
        app:layout_constraintTop_toBottomOf="@+id/protein_title" />

    <EditText
        android:id="@+id/email_input"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:width="280dp"
        android:backgroundTint="@color/brown"
        android:backgroundTintMode="add"
        android:ems="10"
        android:hint="Email Address"
        android:inputType="textEmailAddress"
        android:textColorHint="@color/white"
        android:textSize="22dp"
        app:layout_constraintStart_toStartOf="@+id/last_name_input"
        app:layout_constraintTop_toBottomOf="@+id/last_name_input"></EditText>


    <TextView
        android:id="@+id/calories_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Calories: "
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="@+id/preferences_title"
        app:layout_constraintTop_toBottomOf="@+id/preferences_title" />

    <SeekBar
        android:id="@+id/calories_seekBar_input"
        android:layout_width="140dp"
        android:layout_height="18dp"

        android:layout_marginStart="60dp"
        android:layout_marginTop="16dp"
        android:max="10"
        android:progress="0"
        android:progressBackgroundTint="@color/white"
        android:progressTint="@color/brown"
        android:scaleX="1.2"
        android:scaleY="1.2"
        android:thumbTint="@color/brown"
        app:layout_constraintStart_toEndOf="@+id/calories_title"
        app:layout_constraintTop_toBottomOf="@+id/preferences_title" />

    <TextView
        android:id="@+id/total_fat_title"
        android:layout_width="wrap_content"
